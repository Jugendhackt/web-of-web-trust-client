{
  "name": "Web of Web Trust",
  "description": "Build a trusty web!",
  "version": "0.0.1",
  "manifest_version": 2,
  "options_ui": {
    "page": "options/ui.html",
    "browser_style": true,
    "chrome_style": true
  },
  "permissions": [
    "storage",
    "tabs",
    "notifications"
  ],
  "background": {
    "scripts": [
      "content/main.js"
    ]
  },
  "icons": {
    "512": "media/icon-512.png"
  }
}
